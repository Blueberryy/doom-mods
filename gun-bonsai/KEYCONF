addkeysection "Gun Bonsai" GunBonsai

addmenukey "Display Info" bonsai_show_info
alias bonsai_show_info "netevent bonsai_show_info"
defaultbind I bonsai_show_info

addkeysection "Gun Bonsai/Legendoom Integration" GunBonsaiLegendoom

addmenukey "Cycle Legendoom Weapon Effect" bonsai_cycle_ld_effect
alias bonsai_cycle_ld_effect "netevent bonsai_cycle_ld_effect"
defaultbind P bonsai_cycle_ld_effect
